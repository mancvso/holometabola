    # gain 0.6 # delay 0.2
    # freq 5120
    # stretch sine
    # room 0.2
    # dry 0.1

do
  b6 $ layer [density 0.5, every 4 (fast 2) . slow 2 . gentle] $ struct "[1 [0 1] [1 1] [0 1]]" $ s "~ <uxay:5(2, 8, 1), [uxay:8 uxay:5], uxay:4(6, 8, 2), uxay:9(4, 8), uxay:5(4, 16)>"
