do
  -- 24% -- higher energy
  b3
    $ slow 4
    $ stack [
      density 2 $ s "<bend:5*2, [bend:8 bend:5*3], bend:4*3, bend:9*9, bend:5(4, 16, 2)>"
      # gain 0.75,
      density 2 $ s "<[~ bend:5 ~ ~], [bend:8 bend:8], exxo:5(2, 16, 2)>" # freq 142
      # gain 0.75
    ]
    # cutoff 350
    # freq 400
    # dry 0.6
    # resonance 0.8
    # room 0.9
  -- 39%
